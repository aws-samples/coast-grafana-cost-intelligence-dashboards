            "uid": "${DS_COAST-2023-09-19}"
          },
          "format": 1,
          "rawSQL": "with glue_cost_query as (\nSELECT\nCOUNT(*) as glue_count,\nSUM(line_item_unblended_cost) as glue_cost\nFROM csaws_cur\nWHERE\nline_item_product_code = 'AWSGlue'\nAND line_item_operation = 'CrawlerRun'\nAND line_item_resource_id LIKE '%$CfnStackName%'\nAND (line_item_usage_start_date >= date_add('month', -1, date_trunc('month', current_timestamp)) \n     AND line_item_usage_start_date < date_add('month', -0, date_trunc('month', current_timestamp)) )\n),\n\nathena_cost_query as (\nSELECT\nCOUNT(*) as athena_count,\nSUM(line_item_unblended_cost) as athena_cost\nFROM csaws_cur\nWHERE\nline_item_product_code = 'AmazonAthena'\nAND line_item_resource_id LIKE '%$CfnStackName%'\nAND (line_item_usage_start_date >= date_add('month', -1, date_trunc('month', current_timestamp)) \n     AND line_item_usage_start_date < date_add('month', -0, date_trunc('month', current_timestamp)) )\n),\n\ngrafana_cost_query as (\nSELECT\nCOUNT(*) as grafana_count,\nSUM(line_item_unblended_cost) as grafana_cost\nFROM csaws_cur\nWHERE\nline_item_product_code = 'AmazonGrafana'\nAND line_item_operation LIKE '$GrafanaWorkspace'\nAND (line_item_usage_start_date >= date_add('month', -1, date_trunc('month', current_timestamp)) \n     AND line_item_usage_start_date < date_add('month', -0, date_trunc('month', current_timestamp)) )\n)\n\nSELECT\n(CASE\nWHEN (SELECT glue_count from glue_cost_query) > 0 THEN (SELECT glue_cost from glue_cost_query)\nELSE 0\nEND + \nCASE WHEN (SELECT athena_count from athena_cost_query) > 0 THEN (SELECT athena_cost from athena_cost_query)\nELSE 0\nEND + \nCASE WHEN (SELECT grafana_count from grafana_cost_query) > 0 THEN (SELECT grafana_cost from grafana_cost_query)\nELSE 0\nEND) as cost",
          "refId": "A"
        }
      ],
      "title": "COAST Cost: Previous Month",
      "type": "stat"
    },
